    "# Медведев Георгий; лаб.2, упр.12\n",
    "\n",
    "import turtle as t\n",
    "\n",
    "def arc(): \n",
    "    for i in range(60):   \n",
    "        t.forward(5)\n",
    "        t.left(3)\n",
    "    for i in range(60):   \n",
    "        t.forward(1)\n",
    "        t.left(3)\n",
    "\n",
    "t.left(90)\n",
    "for i in range(10):   \n",
    "    arc()\n",
    "    \n",
    "t.exitonclick()"
